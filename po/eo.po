#: ../frozen-bubble:2633
msgid "Start 1-player game menu"
msgstr "Eku sola-ludo menuon"

#: ../frozen-bubble:2735
msgid "*** Please wait, probing for available servers on local network..."
msgstr "*** B.v. atendi, provante lokan reton por disponeblaj serviloj..."

#: ../frozen-bubble:2738
msgid "*** Unable to probe for available servers on local network!"
msgstr "*** Ne eblis provi lokan reton por disponeblaj serviloj!"

#: ../frozen-bubble:2740
msgid "*** Verify your network setup"
msgstr "*** Kontrolu vian retagordon"

#: ../frozen-bubble:2749
msgid "*** No server found, and could not start server"
msgstr "*** Neniu servilo trovita,kaj ne povis starti servilon "

#: ../frozen-bubble:2750
msgid "*** Verify your installation or contact your vendor"
msgstr "*** Kontrolu vian instalon aŭ kontaktu vian vendiston"

#: ../frozen-bubble:2757
msgid "*** No server found, created server for local network game"
msgstr "*** Neniu servilon trovita, kreis servilon por loka reta ludo"

#: ../frozen-bubble:2773
msgid "*** Contacting master server..."
msgstr "*** Kontaktas mastran servilon..."

#: ../frozen-bubble:2784
msgid "*** Server list received properly"
msgstr "*** Servilo-listo ricevita sukcese"

#: ../frozen-bubble:2786
msgid "*** Unable to download server list from master server!"
msgstr "*** Ne eblis elŝuti servilo-liston el mastra servilon!"

#: ../frozen-bubble:2787
msgid "*** Verify your network setup or retry later"
msgstr "*** Kontrolu vian retan agordon aŭ reprovu poste"

#: ../frozen-bubble:2820
#, perl-format
msgid "Available players:%s  Ping:%sms"
msgstr "Disponeblaj ludantoj:%s  Ping:%sms"

#: ../frozen-bubble:2857
msgid "*** No available game server"
msgstr "*** Neniu disponebla ludoservilo"

#: ../frozen-bubble:2858
msgid "*** Please retry later or try a local network game (lan game)"
msgstr "*** B.v. reprovi poste aŭ provu lokan retan ludon (retludo)"

#: ../frozen-bubble:2865
msgid "*** Please choose a server"
msgstr "*** B.v. elekti servilon"

#: ../frozen-bubble:2926
#, perl-format
msgid "*** Connected to server '%s'"
msgstr "*** Konektita al servilon '%s'"

#: ../frozen-bubble:3042 ../frozen-bubble:3215 ../frozen-bubble:3314
msgid "Chat"
msgstr "Babiladi"

#: ../frozen-bubble:3043
msgid "Create new game"
msgstr "Kreu novan ludon"

#: ../frozen-bubble:3080
msgid "Say: "
msgstr "Diri: "

#: ../frozen-bubble:3093
#, perl-format
msgid "Join game: %s"
msgstr "Aliĝi al ludon: %s"

#: ../frozen-bubble:3108
#, perl-format
msgid "Available Players: %s"
msgstr "Disponeblaj Ludantoj: %s"

#: ../frozen-bubble:3132
msgid "*** Leaving game..."
msgstr "*** Forlasante ludon..."

#: ../frozen-bubble:3203
msgid "*** Game created - now you need to wait for players to join"
msgstr "*** Ludon kreita - nun vi devas atendi aliĝontajn ludantojn"

#: ../frozen-bubble:3205
msgid "*** Joined game"
msgstr "*** Aliĝis al ludon"

#: ../frozen-bubble:3212
msgid "*** Error: game just started or aborted"
msgstr "*** Eraro: ludo ĵus startis aŭ malsukcesis"

#: ../frozen-bubble:3224
#, perl-format
msgid "*** Can't start game: '%s'"
msgstr "*** Ne povas starti ludon: '%s'"

#: ../frozen-bubble:3254
#, perl-format
msgid "*** %s joined the game!"
msgstr "*** %s aliĝis al la ludon!"

#: ../frozen-bubble:3257
#, perl-format
msgid "*** %s left the game..."
msgstr "*** %s forlasis la ludon..."

#: ../frozen-bubble:3300
msgid "*** Lost connection to server, abandoning..."
msgstr "*** Kontakto perdita al servilo, rezignas..."

#: ../frozen-bubble:3313
#, perl-format
msgid "Players in game: %s"
msgstr "Ludantoj en ludo: %s"

#: ../frozen-bubble:3315
msgid "Start game!"
msgstr "Startu ludon!"

#: ../frozen-bubble:3327
#, perl-format
msgid "*** Game finished, because the following player(s) left: %s"
msgstr "*** Ludo finita, ĉar la jena(j) ludanto(j) forlasis: %s"

#: ../frozen-bubble:3331
#, perl-format
msgid "*** Winner: %s"
msgstr "*** Venkinto: %s"

#: ../frozen-bubble:3333
msgid "*** Draw game!"
msgstr "*** Senvenkanta ludo!"

#: ../frozen-bubble:3335
#, perl-format
msgid "*** Scores: %s"
msgstr "*** Poentoj: %s"

#: ../frozen-bubble:3383
msgid "2-player game"
msgstr "2-ludantoj ludo"

#: ../frozen-bubble:3383
msgid "Enable chain-reaction?"
msgstr "Enŝaltu ĉenreagon?"

#: ../frozen-bubble:3384
#, perl-format
msgid "%s or %s?"
msgstr "%s aŭ %s?"

#: ../frozen-bubble:3385
msgid "Enjoy the game!"
msgstr "ĝuu la ludon!"

#: ../frozen-bubble:3423
msgid "Congratulations!"
msgstr "Gratulojn!"

#: ../frozen-bubble:3423
msgid "You have a highscore!"
msgstr "Vi atingis rekordon!"

#: ../frozen-bubble:3424
msgid "Your name?"
msgstr "Vian nomon?"

#: ../frozen-bubble:3425
msgid "Great game!"
msgstr "Bonega ludo!"

#: ../frozen-bubble:3744
msgid "Please enter new keys:"
msgstr "B.v. doni novajn klavojn:"

#: ../frozen-bubble:3746
msgid "Player 1; turn left?"
msgstr "Ludanto 1: maldekstren?"

#: ../frozen-bubble:3747
msgid "Player 1; turn right?"
msgstr "Ludanto 1: dekstren?"

#: ../frozen-bubble:3748
msgid "Player 1; fire?"
msgstr "Ludanto 1: pafu?"

#: ../frozen-bubble:3749
msgid "Player 1; center?"
msgstr "Ludanto 1: mezu?"

#: ../frozen-bubble:3751
msgid "Player 2; turn left?"
msgstr "Ludanto 2: maldekstren?"

#: ../frozen-bubble:3752
msgid "Player 2; turn right?"
msgstr "Ludanto 2: dekstren?"

#: ../frozen-bubble:3753
msgid "Player 2; fire?"
msgstr "Ludanto 2: pafu?"

#: ../frozen-bubble:3754
msgid "Player 2; center?"
msgstr "Ludanto 2: mezu?"

#: ../frozen-bubble:3756
msgid "Toggle fullscreen?"
msgstr "Komutu tutekranon?"

#: ../frozen-bubble:3757
msgid "Chat (net/lan game)?"
msgstr "Babiladu (retludo)?"

#: ../frozen-bubble:3759
msgid "Thanks!"
msgstr "Dankon!"

